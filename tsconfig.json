{
  "compilerOptions": {
    "jsx": "react",
    "allowJs": true,
    "outDir": "./dist/build/",
    "sourceMap": true,
    "noImplicitAny": true,
    "removeComments": true,
    "module": "commonjs",
    "target": "es5",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./app/render/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
